{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "scheme": "frontend",
    "owner": "alirm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-dark.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/icons/ios-dark.png",
        "light": "./assets/icons/ios-light.png",
        "tinted": "./assets/icons/ios-tinted.png"
      },
      "bundleIdentifier": "com.lotuz.frontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lotuz.frontend",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD_CK7jBT5Wwj9UnD1xLr1j7t5zlol4NeE"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.lotuz.frontend",
              "host": "oauth2redirect"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-maps",
        {
          "requestLocationPermission": false,
          "locationPermsission": "Allow Lotuz to access your location"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "80843c05-9ef8-4f46-aa72-75ad671b454c"
      },
      "router": {}
    }
  }
}
